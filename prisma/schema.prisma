generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id           Int      @id @default(autoincrement())
  code         String   @unique
  url          String
  clicks       Int      @default(0)
  last_clicked DateTime?
  created_at   DateTime @default(now())
}
